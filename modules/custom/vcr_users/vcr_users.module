/**
 * Define site roles in code.
 *
 * Create a vcr_users module directory and place this function
 * in vcr_users.module.
 *
 * @return
 *   An array defining all the roles for the site.
 */
function vcr_users_secure_permissions_roles() {
  return array(
    'anonymous user',
    'authenticated user',
    'administrator',
    'content_editor',
    'developer',
    'site_owner',
  );
}

